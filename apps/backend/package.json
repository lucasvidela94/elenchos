{
  "name": "@elenchos/backend",
  "version": "0.1.0",
  "description": "Backend API for Elenchos",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/env": "^4.3.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.0",
    "drizzle-orm": "^0.45.1",
    "ethers": "^6.11.0",
    "fastify": "^4.26.0",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.0",
    "redis": "^4.6.0",
    "viem": "^2.7.0",
    "ws": "^8.16.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/crypto-js": "^4.2.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "drizzle-kit": "^0.31.9",
    "eslint": "^8.57.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vitest": "^1.3.0"
  }
}
